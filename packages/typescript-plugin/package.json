{
  "name": "next-typesafe-url-plugin",
  "version": "0.1.0",
  "description": "TypeScript Language Service Plugin for next-typesafe-url - enables Go to Definition for route strings",
  "license": "MIT",
  "author": "Ethan Niser",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethanniser/next-typesafe-url",
    "directory": "packages/typescript-plugin"
  },
  "keywords": [
    "typescript",
    "plugin",
    "language-service",
    "next-typesafe-url",
    "nextjs",
    "routing"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "devDependencies": {
    "tsup": "7.1.0",
    "typescript": "5.1.3",
    "vitest": "0.32.0"
  }
}
